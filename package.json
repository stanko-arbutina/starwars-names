{
  "name": "@stanko-arbutina/starwars-names",
  "version": "0.0.0-development",
  "description": "Get random Star Wars names",
  "main": "src/index.js",
  "scripts": {
    "test": "istanbul cover -x *.test.js _mocha -- -R spec src/index.test.js",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "semantic-release": "semantic-release",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "commit": "./node_modules/.bin/git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stanko-arbutina/starwars-names.git"
  },
  "keywords": [
    "star",
    "wars",
    "names",
    "tutorial"
  ],
  "author": "Stanko Arbutina <stanko.arbutina@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stanko-arbutina/starwars-names/issues"
  },
  "homepage": "https://github.com/stanko-arbutina/starwars-names#readme",
  "dependencies": {
    "unique-random-array": "2.0.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "codecov.io": "0.1.6",
    "commitizen": "4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "ghooks": "2.0.4",
    "istanbul": "0.4.5",
    "mocha": "7.2.0",
    "semantic-release": "^17.0.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    }
  }
}
